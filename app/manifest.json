{
  "name": "Semantic Scholar Button",
  "short_name": "semantic scholar button",
  "description": "__MSG_appDescription__",
  "version": "0.0.3",
  "manifest_version": 2,
  "default_locale": "en",
  "icons": {
    "16": "images/book-16.png",
    "24": "images/book-24.png",
    "32": "images/book-32.png",
    "64": "images/book-64.png",
    "128": "images/book-128.png"
  },
  "permissions": ["tabs","activeTab","*://www.semanticscholar.org/*"],
  "browser_action": {
    "default_icon": {
      "16": "images/book-16.png",
      "24": "images/book-24.png",
      "32": "images/book-32.png",
      "64": "images/book-64.png",
      "128": "images/book-128.png"
    },
    "__comment": "http://www.freepik.com Creative Commons BY 3.0",
    "default_title": "__MSG_browserActionTitle__",
    "default_popup": "pages/popup.html"
  }
}
